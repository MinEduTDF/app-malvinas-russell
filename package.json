{
  "name": "app-malvinas-russell",
  "version": "0.1.0",
  "description": "Aplicacion web progresiva sobre Malvinas basada en Russell",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "uuid > .amrversion",
    "clean": "rm build/index.html build/assets/*.js build/assets/*.js.gz",
    "makeindex": "node ./makeindex.js `cat .amrversion`",
    "precache": "sw-precache --config=sw-precache-config.json --root=build",
    "build": "npm run version && npm run makeindex && npm run precache && browserify src/index.js | uglifyjs -cm | tee build/assets/bundle-`cat .amrversion`.js | gzip -c > build/assets/bundle-`cat .amrversion`.js.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MinEduTDF/app-malvinas-russell.git"
  },
  "keywords": [
    "russell"
  ],
  "author": "MinEduTDF",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MinEduTDF/app-malvinas-russell/issues"
  },
  "homepage": "https://github.com/MinEduTDF/app-malvinas-russell#readme",
  "dependencies": {
    "JSONStream": "^1.1.1",
    "azimuth": "^0.1.0",
    "csjs": "^1.0.3",
    "csjs-extractify": "^1.0.0-alpha.0",
    "dominant-color": "0.0.1",
    "ecstatic": "^1.4.1",
    "feedme": "^0.3.0",
    "http-proxy": "^1.14.0",
    "hyperstream": "^1.2.2",
    "json-select": "^2.2.0",
    "local-links": "^1.4.0",
    "morphdom": "^2.0.1",
    "oppressor": "0.0.1",
    "request": "^2.72.0",
    "routes": "^2.1.0",
    "russell-view": "^1.0.0",
    "store-emitter": "^2.2.0",
    "sw-precache": "^4.0.0",
    "sw-toolbox": "^3.2.1",
    "uglify-js": "^2.7.3",
    "wayfarer": "^6.1.4",
    "xhr": "^2.2.2",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "csjs-injectify": "^1.0.0",
    "es6-templates": "^0.2.2",
    "sw-toolbox": "^3.2.1",
    "uuid-cli": "^2.0.0",
    "watchify": "^3.7.0",
    "webworkify": "^1.2.1"
  },
  "browserify": {
    "transform": [
      "es6-templates"
    ]
  },
  "browser": {
    "request": "xhr"
  }
}
