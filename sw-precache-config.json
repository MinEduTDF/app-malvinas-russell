{
  "staticFileGlobs": [
    "build",
    "build/mensajes",
    "build/noticias",
    "build/historia",
    "build/azimuth",
    "build/manifest.json",
    "build/assets/**/*"
  ],
  "stripPrefix": "build/",
  "runtimeCaching": [
    {
      "urlPattern": "/\/elmalvinense/",
      "handler": "networkFirst"
    },
    { 
      "urlPattern": "*",
      "handler": "networkFirst"
    }
  ],
    "dontCacheBustUrlsMatching": "assets"
}
