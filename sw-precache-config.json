{
  "staticFileGlobs": [
    "build/index.html",
    "build/mensajes/",
    "build/noticias",
    "build/historia",
    "build/azimuth",
    "build/manifest.json",
    "build/assets/**/*",
    "build/historia/*"
  ],
  "stripPrefix": "build",
  "runtimeCaching": [
    {
      "urlPattern": "/\/elmalvinense/",
      "handler": "networkFirst"
    },
    { 
      "urlPattern": "*",
      "handler": "networkFirst"
    }
  ],
    "dontCacheBustUrlsMatching": "assets/**/*",
    "importScripts": [
      "assets/js/offline-google-analytics-import.js",
      "assets/js/initga.js"
        ]
}
